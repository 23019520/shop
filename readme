-- (Optional) Backup your current data (if needed)
CREATE DATABASE IF NOT EXISTS git_backup_2; -- Use a new name to avoid conflicts
USE git_backup_2;
CREATE TABLE IF NOT EXISTS people SELECT * FROM git.people;
CREATE TABLE IF NOT EXISTS products SELECT * FROM git.products;
CREATE TABLE IF NOT EXISTS orders SELECT * FROM git.orders;
CREATE TABLE IF NOT EXISTS order_history SELECT * FROM git.order_history;

-- Clear tables (DANGER: This deletes all data permanently!)
USE git; -- Switch back to your main database
TRUNCATE TABLE products;
TRUNCATE TABLE orders;
TRUNCATE TABLE order_history;

-- Reset auto-increment counters (optional)
ALTER TABLE products AUTO_INCREMENT = 1;
ALTER TABLE orders AUTO_INCREMENT = 1;
ALTER TABLE order_history AUTO_INCREMENT = 1;

